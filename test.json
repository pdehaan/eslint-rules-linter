{
  "env": {
    "amd": true,
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "globals": {},
  "parser": "/Volumes/Dev/github/foobar/index.js",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "plugins": ["foobar"],
  "rules": {
    "array-bracket-newline": ["off"],
    "array-bracket-spacing": ["off"],
    "array-element-newline": ["off"],
    "arrow-body-style": [0],
    "arrow-parens": ["off"],
    "arrow-spacing": ["off"],
    "block-spacing": ["off"],
    "brace-style": ["off"],
    "camelCase": ["off"],
    "camelcase": [2],
    "comma-dangle": ["off"],
    "comma-spacing": ["off"],
    "comma-style": ["off", "last"],
    "computed-property-spacing": ["off"],
    "consistent-return": [0],
    "constructor-super": ["error"],
    "curly": [0, "all"],
    "dot-location": ["off"],
    "dot-notation": [0],
    "eol-last": ["off"],
    "eqeqeq": [2, "allow-null"],
    "for-direction": ["error"],
    "func-call-spacing": ["off"],
    "function-call-argument-newline": ["off"],
    "function-paren-newline": ["off"],
    "fxa/no-exclusive-tests": [2],
    "generator-star-spacing": ["off"],
    "generator-star": ["off"],
    "getter-return": ["error"],
    "global-strict": [0],
    "handle-callback-err": ["off"],
    "id-blacklist": [2, "self"],
    "implicit-arrow-linebreak": ["off"],
    "indent-legacy": ["off"],
    "indent": [
      "off",
      2,
      {
        "SwitchCase": 1,
        "flatTernaryExpressions": false,
        "offsetTernaryExpressions": false,
        "ignoreComments": false
      }
    ],
    "jsx-quotes": ["off"],
    "key-spacing": ["off"],
    "keyword-spacing": ["off"],
    "linebreak-style": ["off"],
    "lines-around-comment": [0],
    "max-len": [0],
    "multiline-ternary": ["off"],
    "new-cap": [0],
    "new-parens": ["off"],
    "newline-per-chained-call": ["off"],
    "no-arrow-condition": ["off"],
    "no-async-promise-executor": ["error"],
    "no-case-declarations": ["error"],
    "no-class-assign": ["error"],
    "no-comma-dangle": ["off"],
    "no-compare-neg-zero": ["error"],
    "no-cond-assign": [2, "except-parens"],
    "no-confusing-arrow": [0],
    "no-console": ["off"],
    "no-const-assign": ["error"],
    "no-constant-condition": ["error"],
    "no-control-regex": ["error"],
    "no-debugger": [2],
    "no-delete-var": ["error"],
    "no-dupe-args": ["error"],
    "no-dupe-class-members": ["error"],
    "no-dupe-else-if": ["error"],
    "no-dupe-keys": ["error"],
    "no-duplicate-case": ["error"],
    "no-empty-character-class": ["error"],
    "no-empty-pattern": ["error"],
    "no-empty": [0],
    "no-eval": [2],
    "no-ex-assign": ["error"],
    "no-extra-boolean-cast": ["error"],
    "no-extra-parens": ["off"],
    "no-extra-semi": ["off"],
    "no-fallthrough": ["error"],
    "no-floating-decimal": ["off"],
    "no-func-assign": ["error"],
    "no-global-assign": ["error"],
    "no-import-assign": ["error"],
    "no-inner-declarations": ["error"],
    "no-invalid-regexp": ["error"],
    "no-irregular-whitespace": [2],
    "no-loop-func": [0],
    "no-misleading-character-class": ["error"],
    "no-mixed-operators": [0],
    "no-mixed-spaces-and-tabs": ["off"],
    "no-multi-spaces": ["off"],
    "no-multiple-empty-lines": ["off", { "max": 2 }],
    "no-new-symbol": ["error"],
    "no-new": [2],
    "no-obj-calls": ["error"],
    "no-octal": ["error"],
    "no-process-exit": [0],
    "no-prototype-builtins": ["error"],
    "no-redeclare": ["error"],
    "no-regex-spaces": ["error"],
    "no-reserved-keys": ["off"],
    "no-script-url": [2],
    "no-self-assign": ["error"],
    "no-sequences": [2],
    "no-setter-return": ["error"],
    "no-shadow-restricted-names": ["error"],
    "no-shadow": [0],
    "no-space-before-semi": ["off"],
    "no-spaced-func": ["off"],
    "no-sparse-arrays": ["error"],
    "no-tabs": [0],
    "no-this-before-super": ["error"],
    "no-trailing-spaces": ["off"],
    "no-undef": [2],
    "no-underscore-dangle": [0],
    "no-unexpected-multiline": [0],
    "no-unreachable": ["error"],
    "no-unsafe-finally": ["error"],
    "no-unsafe-negation": ["error"],
    "no-unused-labels": ["error"],
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-use-before-define": [2, "nofunc"],
    "no-useless-catch": ["error"],
    "no-useless-escape": ["off"],
    "no-whitespace-before-property": ["off"],
    "no-with": [2],
    "no-wrap-func": ["off"],
    "nonblock-statement-body-position": ["off"],
    "object-curly-newline": ["off"],
    "object-curly-spacing": ["off"],
    "object-property-newline": ["off"],
    "one-var-declaration-per-line": ["off"],
    "operator-linebreak": ["off"],
    "padded-blocks": ["off"],
    "prefer-arrow-callback": [0],
    "prefer-const": [
      2,
      { "destructuring": "any", "ignoreReadBeforeAssign": false }
    ],
    "quote-props": ["off"],
    "quotes": [0, "single", "avoid-escape"],
    "require-yield": ["error"],
    "rest-spread-spacing": ["off"],
    "semi-spacing": ["off"],
    "semi-style": ["off"],
    "semi": ["off", "always"],
    "space-after-function-name": ["off"],
    "space-after-keywords": ["off"],
    "space-before-blocks": ["off"],
    "space-before-function-paren": ["off"],
    "space-before-function-parentheses": ["off"],
    "space-before-keywords": ["off"],
    "space-in-brackets": ["off"],
    "space-in-parens": ["off"],
    "space-infix-ops": ["off"],
    "space-return-throw-case": ["off"],
    "space-unary-ops": ["off", { "words": false, "nonwords": false }],
    "space-unary-word-ops": ["off"],
    "strict": ["off", "function"],
    "switch-colon-spacing": ["off"],
    "template-curly-spacing": ["off"],
    "template-tag-spacing": ["off"],
    "unicode-bom": ["off"],
    "use-isnan": ["error"],
    "valid-typeof": [2],
    "wrap-iife": ["off"],
    "wrap-regex": ["off"],
    "yield-star-spacing": ["off"],
    "yoda": [0]
  },
  "settings": {},
  "ignorePatterns": ["packages/foobar/bin/ejs.js"]
}
